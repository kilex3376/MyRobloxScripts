local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "MY HUB | BLOX FRUITS",
   LoadingTitle = "Loading...",
   ConfigurationSaving = { Enabled = false }
})

local Tab = Window:CreateTab("Main", 4483345998)

-- ТВОЙ СТАРЫЙ АИМБОТ
Tab:CreateToggle({
   Name = "Aimlock Nearest NPC",
   CurrentValue = false,
   Callback = function(Value)
       _G.AimActive = Value
       task.spawn(function()
           while _G.AimActive do
               task.wait()
               pcall(function()
                   local closest = nil
                   local dist = math.huge
                   for _, v in pairs(game.Workspace.Enemies:GetChildren()) do
                       if v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                           local d = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).magnitude
                           if d < dist then
                               dist = d
                               closest = v
                           end
                       end
                   end
                   if closest then
                       workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, closest.HumanoidRootPart.Position)
                   end
               end)
           end
       end)
   end,
})

-- НОВЫЕ ФУНКЦИИ (SPEED / JUMP / DASH)
Tab:CreateSlider({
   Name = "WalkSpeed (Спидхак)",
   Range = {16, 200},
   Increment = 1,
   CurrentValue = 16,
   Callback = function(Value)
       game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

Tab:CreateSlider({
   Name = "JumpPower (Высота прыжка)",
   Range = {50, 300},
   Increment = 1,
   CurrentValue = 50,
   Callback = function(Value)
       game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
       game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
   end,
})

Tab:CreateButton({
   Name = "Infinite Dash (Беск. рывок)",
   Callback = function()
       local old; old = hookmetamethod(game, "__namecall", function(self, ...)
           local method = getnamecallmethod()
           if method == "FireServer" and self.Name == "Dash" then
               -- Логика обхода
           end
           return old(self, ...)
       end)
       print("Dash модифицирован!")
   end,
})
