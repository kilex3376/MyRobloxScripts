local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "MY HUB | BLOX FRUITS",
   LoadingTitle = "Loading...",
   ConfigurationSaving = { Enabled = false }
})

local Tab = Window:CreateTab("Main", 4483345998)

-- СКОРОСТЬ (Увеличил до 500)
Tab:CreateSlider({
   Name = "WalkSpeed (Спидхак)",
   Range = {16, 500},
   Increment = 1,
   CurrentValue = 16,
   Callback = function(Value)
       game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
   end,
})

-- ПРЫЖОК (Увеличил до 500)
Tab:CreateSlider({
   Name = "JumpPower (Высота)",
   Range = {50, 500},
   Increment = 1,
   CurrentValue = 50,
   Callback = function(Value)
       game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
       game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
   end,
})

-- ДАЛЬНОСТЬ ДЭША (Через усиление импульса)
Tab:CreateButton({
   Name = "Buff Dash (Усилить Рывок)",
   Callback = function()
       local UserInputService = game:GetService("UserInputService")
       UserInputService.InputBegan:Connect(function(input, gameProcessed)
           if not gameProcessed and input.KeyCode == Enum.KeyCode.Q then -- Когда жмешь рывок (Q)
               local hum = game.Players.LocalPlayer.Character.Humanoid
               local oldSpeed = hum.WalkSpeed
               hum.WalkSpeed = oldSpeed * 5 -- Временно ускоряем в 5 раз
               task.wait(0.2)
               hum.WalkSpeed = oldSpeed
           end
       end)
       print("Dash buffed!")
   end,
})
